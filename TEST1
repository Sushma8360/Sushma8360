pipeline{
agent any
parameters {
  string defaultValue: 'PROD', name: 'NAME'
}


environment{
TEST="This is TESTING"
}

stages{

stage('A'){

steps{
catchError(buildresult:'SUCCESS', stageResult:'FAILURE'){
echo "PROD ENV"
sh exit 1
}
}
}

stage('B')
{

environment{
TEST1="THIS IS MANUAL"
}

steps{
echo "$TEST1"


}
}
}
}

