pipeline{
agent any
parameters {
  string defaultValue: 'PROD', name: 'NAME'
}

options{
timeouts(time:5,unit:'SECONDS')
}
environment{
TEST="This is TESTING"
}

stages{

stage('A'){

steps{

echo "PROD ENV"


}
}

stage('B')
{

environment{
TEST1="THIS IS MANUAL"
}

steps{
echo "$TEST1"


}
}
}
}

